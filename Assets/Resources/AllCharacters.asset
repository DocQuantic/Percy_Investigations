  m_Script: {fileID: 11500000, guid: d287b4d4ebea9df478d0646a1ac8f56a, type: 3}
  m_Name: Emily Barker
  m_EditorClassIdentifier: 
  characterName: Emily Barker
  idPicture: {fileID: 0}
  description: "Emily Barker est une jeune journaliste du London Daily Chronicles,
    amie d\u2019enfance de Percy. Ce journal est l\u2019un des plus lus de Londres
    et il est donc fr\xE9quent que Percy fasse appel \xE0 son amie pour l\u2019\xE9clairer
    sur les faits marquants actuels et pass\xE9s."
  hash: 1722090787
--- !u!114 &114907302001269248
MonoBehaviour:
  m_ObjectHideFlags: 0
